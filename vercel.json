{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/api/server/:action_level_1",
      "destination": "/api/server"
    },
    {
      "source": "/api/server/:action_level_1/:action_level_2",
      "destination": "/api/server"
    },
    {
      "source": "/api/server/:action_level_1/:action_level_2/:action_level_3",
      "destination": "/api/server"
    }
  ],
  "github": {
    "enabled": false
  }
}
